{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "import"
  ],
  "globals": {
    "process": "readonly"
  },
  "rules": {

    "no-unused-vars": "warn"
  }
}
